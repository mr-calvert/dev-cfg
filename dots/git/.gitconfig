[user]
	email = scalvert@splunk.com
[alias]
	personalprofile = config user.email \"scalvert@onepost.net\"
	co = checkout
	br = branch
	ci = commit
	st = status
	unstage = 'reset HEAD --'
	last = 'log -l HEAD'
[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
